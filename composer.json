{
    "name": "vinceg/asclicenses",
    "description": "Manage ASC.GOV Appraiser Licenses",
    "keywords": ["php", "amc", "asc", "asc.gov"],
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Gabriel",
            "email": "vadimg88@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "guzzlehttp/guzzle": "^6.3",
        "league/csv": "^9.1",
        "symfony/cache": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2",
        "phpstan/phpstan": "^0.10"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "ASCLicenses\\": "src/"
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist"
    }
}
